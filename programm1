meme_dict = {
    "ЛОЛ/КЕК": "что-то смешное",
    "КРИНЖ":"что то странное или стыдное",
    "АБОБУС":"точного значения не существует",
}
word = input("введите слово которое вас интересует(на капс локе):")
if word in meme_dict.keys():
    print(meme_dict[word])
    #НИЧЕГО НЕ РАБОТАЕТЬ(
else:
    print("такого слова нет в базе")
